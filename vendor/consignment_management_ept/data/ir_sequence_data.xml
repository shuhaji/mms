<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    	<!-- Sequence for Consignment Transfer  -->
		<record id="seq_consignment_process" model="ir.sequence">  
        	<field name="name">Sequence Consignment Process</field>  
        	<field name="code">sequence.consignment.process</field>  
        	<field name="prefix">CT</field>
        	<field name="number_increment">1</field>
        	<field name="number_next">1</field>  
        	<field name="padding">3</field>
        	<field name="implementation">standard</field>  
    	</record>
    	
    	<!-- Sequence for Consignment Returns  -->
    	<record id="seq_consignment_return" model="ir.sequence">  
        	<field name="name">Sequence Consignment Return</field>  
        	<field name="code">sequence.consignment.return</field>  
        	<field name="prefix">CR</field>
        	<field name="number_increment">1</field>
        	<field name="number_next">1</field>  
        	<field name="padding">3</field>
        	<field name="implementation">standard</field>  
    	</record>
    	
    	<!-- Sequence for Consignment Internal Transfer  -->
    	<record id="seq_consignment_internal" model="ir.sequence">  
        	<field name="name">Sequence Consignment Internal</field>  
        	<field name="code">sequence.consignment.internal</field>  
        	<field name="prefix">CI/</field>
        	<field name="number_increment">1</field>
        	<field name="number_next">1</field>  
        	<field name="padding">3</field>
        	<field name="implementation">standard</field>  
    	</record>
    	
    	<!-- Sequence for Consignment Log  -->
    	<record id="seq_consignment_log" model="ir.sequence">  
        	<field name="name">Sequence Consignment Log</field>  
        	<field name="code">sequence.consignment.log</field>  
        	<field name="prefix">CML</field>
        	<field name="number_increment">1</field>
        	<field name="number_next">1</field>  
        	<field name="padding">3</field>
        	<field name="implementation">standard</field>  
    	</record>
    	
		<!--     	Create demo(template) file to import consignment transfer/return -->
    	<record id="ir_attachment_import_transfer_return_template_ept" model="ir.attachment">
	        <field name="name">import_transfer_return_template.xls</field>
	        <field name="datas_fname">import_transfer_return_template.xls</field>
	        <field name="datas" type="base64" file="consignment_management_ept/static/xls/import_transfer_return_template.xls"></field>
	        <field name="public">True</field>
	    </record>
	    
	    <!--     	Create demo(template) file to import consignment order -->
	     <record id="ir_attachment_import_order_template_ept" model="ir.attachment">
	        <field name="name">import_order_template.xls</field>
	        <field name="datas_fname">import_order_template.xls</field>
	        <field name="datas" type="base64" file="consignment_management_ept/static/xls/import_order_template.xls"></field>
	        <field name="public">True</field>
	    </record>
	    
	    <!--     	Create demo(template) file to import consignment internal transfer -->
	    <record id="ir_attachment_import_internal_template_ept" model="ir.attachment">
	        <field name="name">import_internal_template.xls</field>
	        <field name="datas_fname">import_internal_template.xls</field>
	        <field name="datas" type="base64" file="consignment_management_ept/static/xls/import_internal_template.xls"></field>
	        <field name="public">True</field>
	    </record>
	    
	    
	    
    </data>
</odoo>  
      