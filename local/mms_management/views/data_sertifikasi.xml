<odoo>
    <data>
        <record id="view_data_sertifikasi_tree" model="ir.ui.view">
            <field name="name">data.sertifikasi.tree</field>
            <field name="model">data.sertifikasi</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Data Sertifikasi">
                    <field name="name"/>
                    <field name="vlk_id"/>
                    <field name="category_vlk"/>
                    <field name="durasi_vlk"/>
                    <field name="masa_awal_sertifikasi"/>
                    <field name="masa_akhir_sertifikasi"/>
                    <field name="penilikan_1"/>
                    <field name="penilikan_2"/>
                    <field name="penilikan_3"/>
                    <field name="penilikan_4"/>
                    <field name="penilikan_5"/>
                    <field name="Status_vlk"/>
                    <field name="ts"/>
                </tree>
            </field>
        </record>

<!--        <record id="seq_permohonan_sertifikasi" model="ir.sequence">-->
<!--            <field name="name">Permohonan Sertifikasi</field>-->
<!--            <field name="code">academic.course.number</field>-->
<!--            <field name="prefix">BQRS/%(y)s/%(month)s/%(day)s/</field>-->
<!--            <field name="padding">3</field>-->
<!--            <field name="company_id" eval="False" />-->
<!--        </record>-->

        <record id="view_data_sertifikasi_form" model="ir.ui.view">
            <field name="name">data.sertifikasi.form</field>
            <field name="model">data.sertifikasi</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Data Sertifikasi">
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <field name="vlk_id"/>
                            <field name="category_vlk"/>
                            <field name="durasi_vlk"/>
                            <field name="masa_awal_sertifikasi"/>
                            <field name="masa_akhir_sertifikasi"/>
                            <field name="penilikan_1"/>
                            <field name="penilikan_2"/>
                            <field name="penilikan_3"/>
                            <field name="penilikan_4"/>
                            <field name="penilikan_5"/>
                            <field name="Status_vlk"/>
                            <field name="ts"/>
                        </group>
                </form>
            </field>
        </record>

    </data>
</odoo>