<odoo>
    <data>
        <record id="view_data_auditor_tree" model="ir.ui.view">
            <field name="name">data.auditor.tree</field>
            <field name="model">data.auditor</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Data auditor">
                    <field name="name"/>
                    <field name="auditor_id"/>
                    <field name="awal_vlk"/>
                    <field name="akhir_vlk"/>
                    <field name="awal_phpl"/>
                    <field name="akhir_phpl"/>
                    <field name="awal_phpl_prod"/>
                    <field name="akhir_phpl_prod"/>
                </tree>
            </field>
        </record>
        <record id="view_data_auditor_form" model="ir.ui.view">
            <field name="name">data.auditor.form</field>
            <field name="model">data.auditor</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Data auditor">
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                    <group>
                        <group>
                            <field name="auditor_id"/>
                            <field name="addres"/>
                            <field name="tempat_lahir"/>
                            <field name="tgl_lahir"/>
                            <field name="agama"/>
                            <field name="gender"/>
                            <field name="ktp"/>
                        </group>
                        <group>
                            <field name="telp"/>
                            <field name="status"/>
                            <field name="awal_vlk"/>
                            <field name="akhir_vlk"/>
                            <field name="awal_phpl"/>
                            <field name="akhir_phpl"/>
                            <field name="awal_phpl_prod"/>
                            <field name="akhir_phpl_prod"/>
                        </group>
                    </group>
                </form>
            </field>
        </record>

    </data>
</odoo>